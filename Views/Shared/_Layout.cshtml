<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Topprem</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/myImages/prem_hfR_icon.ico">
    @Styles.Render("~/Content/css")
    <link href="https://fonts.googleapis.com/css?family=Arimo|Poppins:400,800|Quicksand" rel="stylesheet">
    @RenderSection("Styles", false)
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <div class="container body-content">
        @RenderBody()
        <div class="foot">
        <footer>
            <p>&copy; @DateTime.Now.Year - TOPPREM</p>
        </footer>
    </div>
</div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
